    "version": "0.2.0",
    "configurations": [
        {
            "name": "grog set",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["-v", "trc", "-d", "-set", "John='{\"name\":\"John\", \"surname\":\"Smith\", \"age\":30}'"]
        },
        {
          "name": "grog get",
          "type": "go",
          "request": "launch",
          "mode": "auto",
          "program": "${workspaceFolder}",
          "args": ["-v", "trc", "-d", "-get", "John"]
      }
    ]
}
